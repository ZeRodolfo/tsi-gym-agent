{
  "name": "tsi-gym-agent",
  "version": "1.0.0",
  "main": "public/electron.js",
  "productName": "TSI Gym Agent",
  "homepage": "./",
  "author": {
    "name": "Tsi Gym",
    "email": "gym-suporte@tsitech.com.br"
  },
  "license": "ISC",
  "description": "TSI Gym Agent is a desktop application for managing gym operations.",
  "scripts": {
    "main": "public/electron.js",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "react-scripts start",
    "electron": "electron .",
    "build:react": "react-scripts build",
    "build:app": "npm run build-react && electron-builder",
    "electron-pack": "build --em.main=build/electron.js",
    "preelectron-pack": "yarn build",
    "electron:package:win": "yarn build:react && electron-builder --win --x64 -c.extraMetadata.main=build/electron.js",
    "electron:package:mac": "yarn build:react && electron-builder --mac --x64 -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "yarn build:react && electron-builder --linux --x64 -c.extraMetadata.main=build/electron.js"
  },
  "build": {
    "appId": "br.com.tsigym.agent",
    "productName": "TSI Gym Agent",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "asar": false,
    "win": {
      "target": "nsis",
      "icon": "icons"
    },
    "linux": {
      "icon": "assets/icons",
      "category": "Settings"
    }
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "electron": "^36.0.0",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.3",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@radix-ui/react-icons": "^1.3.2",
    "add": "^2.0.6",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "electron-is-dev": "^3.0.1",
    "express": "^5.1.0",
    "lucide-react": "^0.503.0",
    "radix-ui": "^1.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^7.5.3",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.5",
    "tailwind-animate": "^0.2.10",
    "tailwindcss": "3",
    "tailwindcss-animate": "^1.0.7",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "yarn": "^1.22.22"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
