{
  "name": "tsi-gym-agent",
  "version": "1.0.2",
  "productName": "TSI Gym Agent",
  "homepage": "./",
  "author": {
    "name": "Tsi Gym",
    "email": "gym-suporte@tsitech.com.br"
  },
  "license": "ISC",
  "description": "TSI Gym Agent is a desktop application for managing gym operations.",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "react-scripts start",
    "server": "nodemon build/server/index.js",
    "server:local": "nodemon public/server",
    "build:electron": "tsc -p electron",
    "dist": "npm run build:all && electron-builder --linux --win --x64 -c.extraMetadata.main=build/electron.js --publish never",
    "electron-dev": "concurrently \"npm run start:react\" \"npm run start:electron\"",
    "start:react": "cross-env NODE_ENV=development BROWSER=none react-scripts start",
    "start:electron": "wait-on http://localhost:3001 && electron public/electron.js --no-sandbox",
    "electron:package:win": "npm run build:all && electron-builder --win --x64 -c.extraMetadata.main=build/electron.js",
    "electron:package:mac": "npm run build:all && electron-builder --mac --x64 -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm run build:all && electron-builder --linux --x64 -c.extraMetadata.main=build/electron.js",
    "build:all": "npm run build:react && npm run build:server",
    "build:react": "react-scripts build",
    "build:server": "mkdirp build/server && cpx \"server/**/*\" build/server && cpx .env build/"
  },
  "main": "build/electron.js",
  "build": {
    "publish": null,
    "asar": true,
    "asarUnpack": [
      "**/preload.js",
      "**/*.node"
    ],
    "directories": {
      "buildResources": "public"
    },
    "files": [
      "build/**/*",
      "electron/preload.js"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icons/icon.ico"
    },
    "linux": {
      "icon": "public/icons",
      "category": "Settings",
      "target": [
        "AppImage"
      ]
    },
    "extraResources": [
      {
        "from": "node_modules",
        "to": "build/node_modules",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "cpx": "^1.5.0",
    "electron": "^32.3.3",
    "electron-builder": "^26.0.12",
    "mkdirp": "^3.0.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.3",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@radix-ui/react-icons": "^1.3.2",
    "@tanstack/react-query": "^5.87.4",
    "add": "^2.0.6",
    "axios": "^1.9.0",
    "better-sqlite3": "^11.10.0",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "electron-is-dev": "1.2.0",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "lucide-react": "^0.503.0",
    "node-cron": "^4.2.1",
    "node-machine-id": "^1.1.12",
    "os": "^0.1.2",
    "radix-ui": "^1.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^7.5.3",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.5",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "tailwind-animate": "^0.2.10",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "3",
    "tailwindcss-animate": "^1.0.7",
    "typeorm": "^0.3.26",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "yarn": "^1.22.22"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
