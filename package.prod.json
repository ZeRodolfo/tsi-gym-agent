{
  "name": "tsi-gym-agent",
  "version": "1.0.0",
  "main": "build/electron.js",
  "productName": "TSI Gym Agent",
  "homepage": "./",
  "author": {
    "name": "Tsi Gym",
    "email": "gym-suporte@tsitech.com.br"
  },
  "license": "ISC",
  "description": "TSI Gym Agent is a desktop application for managing gym operations.",
  "scripts": {
    "main": "public/electron.js",
    "server": "nodemon public/server",
    "electron-dev": "concurrently \"cross-env NODE_ENV=production BROWSER=none yarn start\" \"wait-on http://localhost:3001 && electron .\"",
    "start": "react-scripts start",
    "electron": "electron .",
    "build:app": "yarn build:react && electron-builder",
    "build:react": "react-scripts build",
    "electron-pack": "build --em.main=build/electron.js",
    "preelectron-pack": "yarn build",
    "electron:package:win": "yarn build:react && electron-builder --win --x64 -c.extraMetadata.main=build/electron.js",
    "electron:package:mac": "yarn build:react && electron-builder --mac --x64 -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "yarn build:react && electron-builder --linux --x64 -c.extraMetadata.main=build/electron.js"
  },
  "build": {
    "appId": "br.com.tsigym.agent",
    "productName": "TSI Gym Agent",
    "files": [
      "build/**/*",
      "public/electron.js",
      "public/server.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "public"
    },
    "asar": false,
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "linux": {
      "icon": "assets/icons",
      "category": "Settings"
    }
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "electron": "^36.0.0",
    "electron-builder": "^26.0.12",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.3",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@radix-ui/react-icons": "^1.3.2",
    "@tanstack/react-query": "^5.87.4",
    "add": "^2.0.6",
    "axios": "^1.9.0",
    "better-sqlite3": "^11.10.0",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "electron-is-dev": "^3.0.1",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "lucide-react": "^0.503.0",
    "node-cron": "^4.2.1",
    "node-machine-id": "^1.1.12",
    "os": "^0.1.2",
    "radix-ui": "^1.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^7.5.3",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.5",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "tailwind-animate": "^0.2.10",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "3",
    "tailwindcss-animate": "^1.0.7",
    "typeorm": "^0.3.26",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "yarn": "^1.22.22"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
